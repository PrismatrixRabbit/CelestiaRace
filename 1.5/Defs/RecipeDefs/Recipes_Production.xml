<?xml version="1.0" encoding="utf-8"?>
<Defs>
    
    <RecipeDef>
        <defName>Make_CelesBiogelFromOrganics</defName>
        <label>make celestia biogel from organics</label>
        <description>Make a batch of celestia biogel by extracting elements from organic feedstocks.</description>
        <jobString>Refining biogel from organics.</jobString>
        <effectWorking>Cremate</effectWorking>
        <soundWorking>Recipe_Cremate</soundWorking>
        <workAmount>2500</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <allowMixingIngredients>true</allowMixingIngredients>
        <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
        <workSkill>Medicine</workSkill>
        <recipeUsers>
            <li>DrugLab</li>
        </recipeUsers>
        <researchPrerequisite>Celes_Bio_I</researchPrerequisite>
        <ingredients>
            <li>
                <filter>
                    <customSummary>feedstock</customSummary>
                    <categories>
                        <li>Foods</li>
                        <li>PlantMatter</li>
                    </categories>
                    <disallowedCategories>
                        <li>FoodMeals</li>
                    </disallowedCategories>
                </filter>
                <count>3.5</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>Foods</li>
                <li>PlantMatter</li>
            </categories>
            <disallowedThingDefs>
                <li>Hay</li>
                <li MayRequire="Ludeon.RimWorld.Biotech">HemogenPack</li>
                <li MayRequire="Ludeon.RimWorld.Biotech">BabyFood</li>
            </disallowedThingDefs>
            <disallowedCategories>
                <li>FoodMeals</li>
            </disallowedCategories>
        </fixedIngredientFilter>
        <defaultIngredientFilter>
            <categories>
                <li>FoodRaw</li>
            </categories>
            <disallowedCategories>
                <li>MeatRaw</li>
                <li>AnimalProductRaw</li>
            </disallowedCategories>
        </defaultIngredientFilter>
        <products>
            <CelesBioGel>20</CelesBioGel>
        </products>
    </RecipeDef>


<?xml version="1.0" encoding="utf-8"?>
<Defs>

<FactionDef ParentName="PlayerFactionBase">
	<defName>Celes_PlayerFaction</defName>
	<label>Celestia new outpost</label>
	<description>A celestia new outpost~</description><!--等我写（-->
	<settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
	<categoryTag>Celes_PlayerFaction</categoryTag>
	<colorSpectrum>
      <li>(0.58, 0.8, 0.92)</li>
    </colorSpectrum>
    <isPlayer>true</isPlayer>
	<basicMemberKind>Celes_Colonist</basicMemberKind>
	<pawnSingular>celestia colonist</pawnSingular>
    <pawnsPlural>celestia colonists</pawnsPlural>
	<techLevel>Industrial</techLevel>
	<melaninRange>0</melaninRange>
	<!--↓↓↓这里等着改↓↓↓-->
	<!--NameMaker都等我写-->
	<playerInitialSettlementNameMaker>CelesInitialSettlement_Namer</playerInitialSettlementNameMaker>
	<factionNameMaker>CelesFaction_Namer</factionNameMaker>
    <settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
    <allowedCultures>
		<li>CelesCulture</li>
	</allowedCultures>
	<!--<disallowedMemes>
			<li MayRequire="Ludeon.RimWorld.Ideology">Blindsight</li>
    </disallowedMemes>-->
	<backstoryFilters>
      <li>
        <categories>
          <li>Celes_Space</li>
        </categories>
      </li>
    </backstoryFilters>
	<factionIconPath>Celes/Faction/CelesFaction_Icon</factionIconPath><!--这里要换兔兔的地图标识-->
	<startingResearchTags>
      <li>ClassicStart</li>
    </startingResearchTags>
    <startingTechprintsResearchTags>
      <li>ClassicStart</li>
      <li>ClassicStartTechprints</li>
    </startingTechprintsResearchTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
      </thingDefs>
	  <disallowedThingDefs>
        <li>Hyperweave</li>
      </disallowedThingDefs>
    </apparelStuffFilter>
	<!--↑↑↑这里等着改↑↑↑-->
</FactionDef>

<FactionDef ParentName="FactionBase">
	<defName>Celes_Faction</defName>
	<label>celestia pioneer</label>
    <description>A group of celestia pioneers?</description><!--等我写（-->
	<colorSpectrum>
      <li>(1, 1, 1)</li><!--等我改（-->
    </colorSpectrum>
	<categoryTag>Celes_Faction</categoryTag>
	<pawnSingular>celestia</pawnSingular>
    <pawnsPlural>celestias</pawnsPlural>
	<listOrderPriority>30</listOrderPriority><!--这个可能小了要改，原版30，kiiro是240-->
	<settlementGenerationWeight>1</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
	<maxConfigurableAtWorldCreation>1</maxConfigurableAtWorldCreation>
	<canMakeRandomly>true</canMakeRandomly>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
	<leaderTitle>pioneer commander-in-chief</leaderTitle><!--等我写（-->
    <factionIconPath>Celes/Faction/CelesFaction_Icon</factionIconPath><!--派系图标丢了-->
    <techLevel>Industrial</techLevel>
	<!--NameMaker都等我写-->
    <factionNameMaker>CelesFaction_Namer</factionNameMaker>
    <settlementNameMaker>CelesSettlement_Namer</settlementNameMaker>
	<melaninRange>0</melaninRange>
    <naturalEnemy>false</naturalEnemy>
    <permanentEnemy>false</permanentEnemy>
    <allowedCultures><li>CelesCulture</li></allowedCultures>
	<!--↓↓↓这里等着改↓↓↓-->
	<!--什么异种啊文化啊戒律啊麻烦死了先占位-->
	<requiredMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Individualist</li>
    </requiredMemes>
	<!-- 这是白名单和黑名单，不能共用，到时候再按需自取 -->
    <!-- <allowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">FleshPurity</li>
    </allowedMemes>
	<disallowedMemes>
      <li MayRequire="Ludeon.RimWorld.Ideology">Structure_Animist</li>
    </disallowedMemes> -->
	<structureMemeWeights>
      <Structure_TheistEmbodied MayRequire="Ludeon.RimWorld.Ideology">1</Structure_TheistEmbodied>
    </structureMemeWeights>
    <disallowedPrecepts>
      <li MayRequire="Ludeon.RimWorld.Ideology">Lovin_Prohibited</li>
    </disallowedPrecepts>
	<xenotypeSet>
      <xenotypeChances>
	    <Baseliner MayRequire="Ludeon.RimWorld.Biotech">1</Baseliner>
      </xenotypeChances>
    </xenotypeSet>
	<!--↑↑↑这里等着改↑↑↑-->
	<backstoryFilters>
      <li>
            <categories>
                 <li>Celes_Outlander</li>
            </categories>
            <commonality>0.75</commonality>
      </li>
      <li>
            <categories>
                <li>Celes_Space</li>
            </categories>
            <commonality>0.25</commonality>
      </li>
        <!--<li>
               <categories>
                    <li>Celes_Military</li>这个留着的意思是背景可以在好几个里面随机，但我目前只写了一个
             </categories>
             <commonality>0.05</commonality>
      </li>-->
    </backstoryFilters>
	<!--衣服可能要限制材质？-->
	<!--<apparelStuffFilter>
      <stuffCategoriesToAllow>
      </stuffCategoriesToAllow>
      <disallowedThingDefs>
      </disallowedThingDefs>
    </apparelStuffFilter>-->
	<!--商队不太懂哦-->
	<caravanTraderKinds>
      <li>Caravan_Outlander_BulkGoods</li>
      <li>Caravan_Outlander_CombatSupplier</li>
      <li>Caravan_Outlander_Exotic</li>
      <li>Caravan_Outlander_PirateMerchant</li>
    </caravanTraderKinds>
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
	<allowedArrivalTemperatureRange>-15~40</allowedArrivalTemperatureRange>
	<maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
	<!--这里抄了原版-->
	<pawnGroupMakers>
		<!-- 注释掉的是这个需要的pawn我们没写 -->
		<!-- Normal -->
        <li>
            <kindDef>Combat</kindDef>
            <commonality>100</commonality>
            <options>
                <Celes_Scout>10</Celes_Scout>
            </options>
        </li>
        <!-- Normal-2 -->
        <li>
            <kindDef>Combat</kindDef>
            <commonality>30</commonality>
            <options>
                <Celes_Scout>10</Celes_Scout>
                <Celes_Outlander>5</Celes_Outlander>
            </options>
        </li>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <Celes_Outlander>10</Celes_Outlander>
        </options>
      </li>
      <li>
        <kindDef>Trader</kindDef>
        <traders>
          <Celes_Trader>1</Celes_Trader>
        </traders>
        <carriers>
          <Celes_Outlander>10</Celes_Outlander>
        </carriers>
        <guards>
          <Celes_Scout>10</Celes_Scout>
        </guards>
      </li>
      <li>
        <kindDef>Settlement</kindDef>
        <options>
          <Celes_Scout>10</Celes_Scout>
        </options>
      </li>
      <!--<li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Miners</kindDef>
        <commonality>1</commonality>
        <options>
          <Miner>1</Miner>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Hunters</kindDef>
        <commonality>1</commonality>
        <options>
          <Hunter>1</Hunter>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Loggers</kindDef>
        <commonality>1</commonality>
        <options>
          <Logger>1</Logger>
        </options>
      </li>
      <li MayRequire="Ludeon.RimWorld.Ideology">
        <kindDef>Farmers</kindDef>
        <commonality>1</commonality>
        <options>
          <Farmer>1</Farmer>
        </options>
      </li>-->
    </pawnGroupMakers>
    <settlementTexturePath>World/WorldObjects/DefaultSettlement</settlementTexturePath>
    <modExtensions>
        <li Class="CelesFeature.ModExtension_Aid">
            <greetWords>
                <li>
                    <key>-50~50</key>                        <!-- 好感度区间，如果派系的好感度在这个词条下找不到则不会显示支援 -->
                    <value>GreetI</value>
                </li>
                <li>
                    <key>50~100</key>                        <!-- 好感度区间，如果派系的好感度在这个词条下找不到则不会显示支援 -->
                    <value>GreetII</value>
                </li>
            </greetWords>
        </li>
    </modExtensions>
</FactionDef>

</Defs>
    <!-- ========== 制衣标准 ========= -->

    <ThingDef ParentName="Celes_Underwear_I"> <!--星铃内衣A款--> <!--待编辑-->
        <defName>Celes_UnderwearA</defName>
        <label>celestia underwear A</label>
        <description>celestia underwear A.</description>
        <costStuffCount>20</costStuffCount>
        <stuffCategories>
            <li>Fabric</li>
            <li>Leathery</li>
        </stuffCategories>
        <graphicData>
            <texPath>Celes/Apparel/Underwear/UnderwearA/UnderwearA</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <Mass>0.2</Mass>
            <MaxHitPoints>100</MaxHitPoints>
            <WorkToMake>150</WorkToMake>
            <StuffEffectMultiplierArmor>0.01</StuffEffectMultiplierArmor>
            <StuffEffectMultiplierInsulation_Cold>0.05</StuffEffectMultiplierInsulation_Cold>
            <StuffEffectMultiplierInsulation_Heat>0.05</StuffEffectMultiplierInsulation_Heat>
        </statBases>
        <apparel>
            <useWornGraphicMask>true</useWornGraphicMask>
            <developmentalStageFilter>Adult</developmentalStageFilter>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Legs</li>
            </bodyPartGroups>
            <wornGraphicPath>Celes/Apparel/Underwear/UnderwearA/UnderwearA</wornGraphicPath>
        </apparel>
        <tradeTags>
            <li>Celes_Clothing</li>
        </tradeTags>
    </ThingDef>


                <Crafting>4</Crafting>
                <Intellectual>5</Intellectual>
            </skillRequirements>
            <bulkRecipeCount>5</bulkRecipeCount>
        </recipeMaker>
        <allowedArchonexusCount>10</allowedArchonexusCount>
    </ThingDef>
    
